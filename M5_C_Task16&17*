#include <stdio.h>

float circleArea(float radius);
float elipseArea (float axisA, float axisB);
int main()
{
    float radius = 1.0;
    float axisA, axisB = 0;
    
    printf("%f\n", circleArea(radius));
    radius = 1.5;
    printf("%f\n", circleArea(radius));
    radius = 13.0;
    printf("%f\n", circleArea(radius));

return 0;
}

float circleArea(float radius)
{
    const float Pi = 3.14159;
    return radius*radius*Pi;
}

float elipseArea (float axisA, float axisB)
{
  const float Pi = 3.14159;
  return axisA*axisB*Pi;  
}
